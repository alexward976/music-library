{
  "swagger": "2.0",
  "info": {
    "title": "Music Library API",
    "description": "An API for storing songs and creating a personal playlist.",
    "version": "1.0.0"
  },
  "host": "music-library-72kl.onrender.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "paths": {
    "/library/": {
      "get": {
        "description": "Gets all the songs in the music library. No user required.",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/library/{id}": {
      "get": {
        "description": "Gets a song by ID. No user required.",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/library/add": {
      "post": {
        "description": "Adds a song to the library. No user required.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "title": {
                  "example": "Don't Stop Believin'"
                },
                "artist": {
                  "example": "Journey"
                },
                "album": {
                  "example": "Escape"
                },
                "duration": {
                  "example": "4:10"
                },
                "release_year": {
                  "example": "1981"
                },
                "single": {
                  "example": false
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "412": {
            "description": "Precondition Failed"
          }
        }
      }
    },
    "/library/update/{id}": {
      "put": {
        "description": "Update a specific song in the library. No user required.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "title": {
                  "example": "Bohemian Rhapsody"
                },
                "artist": {
                  "example": "Queen"
                },
                "album": {
                  "example": "A Night at the Opera"
                },
                "duration": {
                  "example": "5:55"
                },
                "release_year": {
                  "example": "1975"
                },
                "single": {
                  "example": false
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "412": {
            "description": "Precondition Failed"
          }
        }
      }
    },
    "/library/delete/{id}": {
      "delete": {
        "description": "Deletes a specific song from the library. No user required.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/playlist/": {
      "get": {
        "description": "Gets all the songs in the user's playlist. Must be logged in.",
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/playlist/{id}": {
      "get": {
        "description": "Gets a specific song in the user's playlist. Must be logged in.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/playlist/add": {
      "post": {
        "description": "Adds a song to the user's playlist. Must be logged in.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "title": {
                  "example": "Rocket Man"
                },
                "artist": {
                  "example": "Elton John"
                },
                "album": {
                  "example": "Honky Ch√¢teau"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          },
          "412": {
            "description": "Precondition Failed"
          }
        }
      }
    },
    "/playlist/update/{id}": {
      "put": {
        "description": "Update a specific song in the user's playlist. Must be logged in.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "title": {
                  "example": "Piano Man"
                },
                "artist": {
                  "example": "Billy Joel"
                },
                "album": {
                  "example": "Piano Man"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          },
          "412": {
            "description": "Precondition Failed"
          }
        }
      }
    },
    "/playlist/delete/{id}": {
      "delete": {
        "description": "Deletes a specific song from the uesr's playlist. Must be logged in.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    }
  }
}